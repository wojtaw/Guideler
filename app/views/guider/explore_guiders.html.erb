<h1 class="profile-h1">Explore Guiders</h1>
<br>
<div class="clearfix">
  <% guiders.each do |g| %>
      <div class="profile-guiderBox">
        <div class="profile-guiderImage"></div>
        <%= g.id %> / <%= g.name %><br>
        <%= link_to('Watch', g_path(:guiderID => g.id),:target => "_blank") %>
      </div>
  <% end %>
</div>